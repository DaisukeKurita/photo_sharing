<h1>Sing up</h1>
<%= form_with(model: @user, local: true) do |f| %>
  <%= f.label :user_image %>
  <%= f.file_field :user_image %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :password %>
  <%= f.text_field :password %>
  <%= f.label :password_confirmation %>
  <%= f.text_field :password_confirmation %>
  <%= f.submit "Create my account" %>
<% end %>
